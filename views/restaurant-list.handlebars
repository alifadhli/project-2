{{#each restaurants}}
<div class="strip_list wow fadeIn" data-wow-delay="0.1s">
	<div class="row">
		<div class="col-md-9 col-sm-9">
			<div class="desc">
				<h3>{{name}}</h3>
				<div class="location">
					{{city}}
				</div>
			</div>
		</div>
	</div><!-- End row-->
</div><!-- End strip_list-->
{{/each}}